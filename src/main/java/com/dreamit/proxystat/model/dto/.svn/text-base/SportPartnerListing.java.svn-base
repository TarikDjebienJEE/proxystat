package com.dreamit.proxystat.model.dto;

import java.util.Date;

import com.dreamit.proxystat.model.entities.Equipment;

/**
 * Represent a sport partner list of equipment to prepare
 * @author acraske
 *
 */
public class SportPartnerListing {

	private Equipment equipment;
	
	private Date preparationDate;
	
	private Long quantity;

	public Equipment getEquipment() {
		return equipment;
	}

	public void setEquipment(Equipment equipment) {
		this.equipment = equipment;
	}

	public Date getPreparationDate() {
		return preparationDate;
	}

	public void setPreparationDate(Date preparationDate) {
		this.preparationDate = preparationDate;
	}

	public Long getQuantity() {
		return quantity;
	}

	public void setQuantity(Long quantity) {
		this.quantity = quantity;
	}

}
