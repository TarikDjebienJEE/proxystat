package com.dreamit.proxystat.model.exceptions;

/**
 * Exception à utiliser quand l'un des champs d'une entité à crée est non présent mais requis
 * @author acraske
 *
 */
public class MissingRequiredInformationException extends Exception {

	/**
	 * generated serialVersionUID
	 */
	private static final long serialVersionUID = 6434832767032258703L;

	/**
	 * Default constructor
	 */
	public MissingRequiredInformationException() {
		super("At least one or more information is missing to work with an object");
	}
	
	/**
	 * constructor pouvant etre utilise depuis appel pour set le message
	 * @param message a attacher a l'exception
	 */
	public MissingRequiredInformationException(String message) {
		super(message);
	}
	
}
